
# setenv GUILE_LOAD_PATH /sw/share/guile/1.6.4
# setenv DYLD_LIBRARY_PATH /usr/local/lib:/Users/paulemsley/autobuild/Darwin-Paris.local-pre-release/lib

set guile_config_libdir = `guile-config info libdir`
set guile_config_pkgdatadir = `guile-config info pkgdatadir`

setenv LD_LIBRARY_PATH $guile_config_libdir 
setenv GUILE_LOAD_PATH $guile_config_pkgdatadir




